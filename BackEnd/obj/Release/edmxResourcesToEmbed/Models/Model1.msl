          <ScalarProperty Name="password" ColumnName="password" />
          <ScalarProperty Name="num_of_action" ColumnName="num_of_action" />
        </MappingFragment>
      </EntityTypeMapping>
    </EntitySetMapping>
  </EntityContainerMapping>
</Mapping>